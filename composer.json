{
    "name": "leaf/sida-pasca",
    "description": "Sistem Informasi & Data Admisi Pasca ULM",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "leafs/leaf": "^3.0",
        "leafs/blade": "^2.0",
        "leafs/db": "^2.0",
        "leafs/auth": "^2.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "dompdf/dompdf": "^2.0",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
